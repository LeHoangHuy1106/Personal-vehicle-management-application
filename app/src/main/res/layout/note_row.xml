        app:layout_constraintStart_toStartOf="@+id/guideline81"
        app:layout_constraintTop_toTopOf="@+id/guideline77" />

    <TextView
        android:id="@+id/txtGiaTien"
        android:layout_width="0dp"
        android:layout_height="0dp"
